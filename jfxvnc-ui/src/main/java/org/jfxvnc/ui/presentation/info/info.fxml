<TitledPane maxWidth="1.7976931348623157E308" text="%info.header" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.jfxvnc.ui.presentation.info.InfoViewPresenter">
	<content>
		<ScrollPane fitToWidth="true" maxWidth="1.7976931348623157E308" prefHeight="500.0" prefViewportWidth="400.0">
			<content>
				<GridPane prefHeight="500.0">
