						<ColumnConstraints hgrow="ALWAYS" maxWidth="1.7976931348623157E308" minWidth="100.0" prefWidth="250.0" />
					</columnConstraints>
					<rowConstraints>
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
						<RowConstraints minHeight="10.0" prefHeight="20.0" vgrow="SOMETIMES" />
						<RowConstraints minHeight="10.0" prefHeight="20.0" vgrow="SOMETIMES" />
						<RowConstraints minHeight="10.0" prefHeight="20.0" vgrow="SOMETIMES" />
						<RowConstraints minHeight="10.0" prefHeight="20.0" vgrow="SOMETIMES" />
						<RowConstraints minHeight="10.0" prefHeight="20.0" vgrow="SOMETIMES" />
						<RowConstraints minHeight="10.0" prefHeight="20.0" vgrow="SOMETIMES" />
						<RowConstraints minHeight="10.0" prefHeight="20.0" vgrow="SOMETIMES" />
						<RowConstraints minHeight="10.0" prefHeight="20.0" vgrow="SOMETIMES" />
						<RowConstraints minHeight="10.0" prefHeight="20.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
						<RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
						<RowConstraints minHeight="10.0" prefHeight="20.0" vgrow="SOMETIMES" />
						<RowConstraints minHeight="10.0" prefHeight="20.0" vgrow="SOMETIMES" />
						<RowConstraints minHeight="10.0" prefHeight="20.0" vgrow="SOMETIMES" />
						<RowConstraints minHeight="10.0" prefHeight="20.0" vgrow="SOMETIMES" />
						<RowConstraints minHeight="10.0" prefHeight="20.0" vgrow="SOMETIMES" />
						<RowConstraints minHeight="10.0" prefHeight="20.0" vgrow="SOMETIMES" />
						<RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
						<RowConstraints minHeight="10.0" prefHeight="20.0" vgrow="SOMETIMES" />
						<RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="20.0" vgrow="SOMETIMES" />
					</rowConstraints>
					<padding>
						<Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
					</padding>
					<children>

						<Label text="Desktop name:" textFill="#686868" GridPane.rowIndex="1" />
						<Label text="Desktop size:" textFill="#686868" GridPane.rowIndex="2" />
						<Label text="VNC server host:" textFill="#686868" GridPane.rowIndex="3" />
						<Label text="Pixelformat:" textFill="#686868" GridPane.rowIndex="4" />
						<Label text="Pixelformat (server):" textFill="#686868" GridPane.rowIndex="5" />
						<Label text="Encoding:" textFill="#686868" GridPane.rowIndex="6" />
						<Label text="RFB protocol version:" textFill="#686868" GridPane.rowIndex="7" />
						<Label text="Security:" textFill="#686868" GridPane.rowIndex="8" />
						<Label text="Connection type:" textFill="#686868" GridPane.rowIndex="9" />
						
						<Label fx:id="infoName" maxWidth="1.7976931348623157E308" minWidth="50.0" prefWidth="150.0" text="-" wrapText="true" GridPane.columnIndex="1" GridPane.hgrow="ALWAYS" GridPane.rowIndex="1" />
						<Label fx:id="infoSize" maxWidth="1.7976931348623157E308" minWidth="50.0" prefWidth="150.0" text="-" GridPane.columnIndex="1" GridPane.hgrow="ALWAYS" GridPane.rowIndex="2" />
						<Label fx:id="infoHost" maxWidth="1.7976931348623157E308" minWidth="50.0" prefWidth="150.0" text="-" wrapText="true" GridPane.columnIndex="1" GridPane.hgrow="ALWAYS" GridPane.rowIndex="3" />
						<Label fx:id="infoPixelformat" maxWidth="1.7976931348623157E308" minWidth="50.0" prefWidth="150.0" text="-" wrapText="true" GridPane.columnIndex="1" GridPane.hgrow="ALWAYS" GridPane.rowIndex="4" />
						<Label fx:id="infoPixelformatDef" maxWidth="1.7976931348623157E308" minWidth="50.0" prefWidth="150.0" text="-" wrapText="true" GridPane.columnIndex="1" GridPane.hgrow="ALWAYS" GridPane.rowIndex="5" />
						<Label fx:id="infoEncoding" maxWidth="1.7976931348623157E308" minWidth="50.0" prefWidth="150.0" text="-" wrapText="true" GridPane.columnIndex="1" GridPane.hgrow="ALWAYS" GridPane.rowIndex="6" />
						<Label fx:id="infoProtocol" maxWidth="1.7976931348623157E308" minWidth="50.0" prefWidth="150.0" text="-" wrapText="true" GridPane.columnIndex="1" GridPane.hgrow="ALWAYS" GridPane.rowIndex="7" />
						<Label fx:id="infoSecurity" maxWidth="1.7976931348623157E308" minWidth="50.0" prefWidth="150.0" text="-" wrapText="true" GridPane.columnIndex="1" GridPane.hgrow="ALWAYS" GridPane.rowIndex="8" />
						<Label fx:id="infoConnectType" maxWidth="1.7976931348623157E308" minWidth="50.0" prefWidth="150.0" text="-" wrapText="true" GridPane.columnIndex="1" GridPane.hgrow="ALWAYS" GridPane.rowIndex="9" />

						<Label text="%statistics.header" GridPane.rowIndex="11">
							<font>
								<Font name="System Bold" size="12.0" />
							</font>
						</Label>

						<Label text="Raw Rects:" textFill="#686868" GridPane.rowIndex="12" />
						<Label text="Copy Rects:" textFill="#686868" GridPane.rowIndex="13" />
						<Label text="Hextile Rects:" textFill="#686868" GridPane.rowIndex="14" />
						<Label text="Zlib Rects:" textFill="#686868" GridPane.rowIndex="15" />
						<Label text="Cursor:" textFill="#686868" GridPane.rowIndex="16" />
						<Label text="Desktop Resize:" textFill="#686868" GridPane.rowIndex="17" />
						<Label text="Total Rects:" textFill="#686868" GridPane.rowIndex="18" />


						<Label fx:id="rawrect" maxWidth="1.7976931348623157E308" minWidth="50.0" prefWidth="150.0" text="-" GridPane.columnIndex="1" GridPane.hgrow="ALWAYS" GridPane.rowIndex="12" />
						<Label fx:id="copyrect" maxWidth="1.7976931348623157E308" minWidth="50.0" prefWidth="150.0" text="-" wrapText="true" GridPane.columnIndex="1" GridPane.hgrow="ALWAYS" GridPane.rowIndex="13" />
