						<Label fx:id="desktop" maxWidth="1.7976931348623157E308" minWidth="50.0" prefWidth="150.0" text="-" wrapText="true" GridPane.columnIndex="1" GridPane.hgrow="ALWAYS" GridPane.rowIndex="17" />
						<Label fx:id="total" maxWidth="1.7976931348623157E308" minWidth="50.0" prefWidth="150.0" text="-" wrapText="true" GridPane.columnIndex="1" GridPane.hgrow="ALWAYS" GridPane.rowIndex="18" />

						<CheckBox fx:id="enableCB" mnemonicParsing="false" GridPane.columnIndex="1" GridPane.rowIndex="20" />
						<Button mnemonicParsing="false" onAction="#reset" text="Reset" GridPane.rowIndex="20" />
                  <Separator prefWidth="200.0" GridPane.columnSpan="2" GridPane.rowIndex="10" />
                  <Label text="%server.details">
                     <font>
                        <Font name="System Bold" size="12.0" />
                     </font>
                  </Label>



					</children>
				</GridPane>
			</content>
		</ScrollPane>
	</content>
</TitledPane>
